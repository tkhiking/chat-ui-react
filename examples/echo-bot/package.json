    "@testing-library/react": "10.4.9",
    "@testing-library/user-event": "12.1.3",
    "@tkhiking/eslint-config": "0.1.9",
    "@tkhiking/node-toolchain": "0.1.9",
    "@types/jest": "26.0.10",
    "@types/node": "14.6.0",
    "@types/react": "16.9.47",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "chat-ui-react": "0.2.8",
    "eslint": "7.7.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "eslint-plugin-unicorn": "21.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.3",
    "typescript": "4.0.2"
  },
  "scripts": {
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "lint": "eslint src",
    "format": "eslint --fix src",
    "test": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts test",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,mjs,jsx,ts,tsx}": "eslint",
    "*.{css}": "stylelint"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@tkhiking"
  },
  "stylelint": {
    "extends": "@tkhiking/stylelint-config"
  },
  "prettier": "@tkhiking/prettier-config",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
